{
	"name": "vscode-libra-move",
	"displayName": "Libra Move Language VSCode",
	"description": "Libra Move language (syntax highlighting, snippets) for Visual Studio Code",
	"version": "0.0.10",
	"publisher": "SDKBox",
	"engines": {
		"vscode": "^1.20.0"
	},
	"keywords": [
		"libra",
		"Libra",
		"Move",
		"move",
		"extension",
		"autocompletion"
	],
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"main": "./out/src/extension",
	"contributes": {
		"languages": [
			{
				"id": "mvir",
				"extensions": [
					".mvir"
				],
				"aliases": [
					"Move"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"snippets": [
			{
				"language": "mvir",
				"path": "./snippets.json"
			}
		],
		"grammars": [
			{
				"language": "mvir",
				"scopeName": "source.mvir",
				"path": "./syntaxes/mvir.tmGrammar.json"
			}
		],
		"commands": [
			{
				"command": "move.compile",
				"title": "Move - Compile current file"
			}
		]
	},
	"repository": "https://github.com/sdkbox/vscode-libra-move",
	"icon": "logo.png",
	"activationEvents": [
		"onCommand:move.compile"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"package": "vsce package",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"encoding": "^0.1.12",
		"lodash": ">=4.17.13",
		"vsce": "^1.66.0"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.43",
		"mocha": "^6.1.4",
		"typescript": "^2.5.2",
		"vscode": "^1.1.5"
	}
}
